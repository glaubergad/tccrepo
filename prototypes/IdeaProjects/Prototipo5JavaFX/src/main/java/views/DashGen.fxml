<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="principalAnchorPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="500.0" minWidth="500.0"
            prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.172-ea"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="DashGenController">
    <children>
        <Label layoutX="169.0" layoutY="25.0" text="IFPA Dashgen">
            <font>
                <Font name="System Bold" size="25.0"/>
            </font>
        </Label>
        <Button id="btnSelectCSV" fx:id="btnSelectCSV" layoutX="14.0" layoutY="97.0" mnemonicParsing="false"
                onAction="#selectCSV" prefHeight="25.0" prefWidth="115.0" text="Selecionar CSV"/>
        <Button fx:id="btnSelectDest" layoutX="14.0" layoutY="161.0" mnemonicParsing="false" onAction="#selectDest"
                text="Selecionar Destino"/>
        <Label layoutX="14.0" layoutY="73.0" text="Local..."/>
        <Label fx:id="lblPathDest" layoutX="14.0" layoutY="138.0" text="Local..."/>
        <ComboBox fx:id="cbAtributo" layoutX="14.0" layoutY="270.0" prefHeight="25.0" prefWidth="184.0"
                  promptText="Atributo"/>
        <ComboBox fx:id="cbTipoGrafico" layoutX="14.0" layoutY="334.0" prefHeight="25.0" prefWidth="184.0"
                  promptText="Tipo de Grafico"/>
        <TextField fx:id="tfTituloDashboard" layoutX="15.0" layoutY="225.0" prefHeight="25.0" prefWidth="457.0"/>
        <TextField fx:id="tfTituloGrafico" layoutX="15.0" layoutY="396.0" prefHeight="25.0" prefWidth="184.0"/>
        <Label layoutX="14.0" layoutY="198.0" text="Título do Dashboard"/>
        <Label layoutX="14.0" layoutY="372.0" text="Titulo do Grafico"/>
        <Button fx:id="btnAddGrafico" layoutX="53.0" layoutY="461.0" mnemonicParsing="false" onAction="#addGraficoClick"
                text="Adicionar Gráfico"/>
        <ListView layoutX="261.0" layoutY="270.0" prefHeight="153.0" prefWidth="211.0"/>
        <Button fx:id="btnFinishDashboard" layoutX="306.0" layoutY="461.0" mnemonicParsing="false"
                onAction="#finishDashboardClick" text="Finalizar Dashboard"/>
    </children>
</AnchorPane>
